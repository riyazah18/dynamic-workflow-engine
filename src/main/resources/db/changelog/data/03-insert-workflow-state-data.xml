<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">


<changeSet id="1000000003" author="riyaz.ahmed">
        <insert tableName="workflow_state">
            <column name="name" value="Initiated"/>
            <column name="workflow_id" value="1"/>
            <column name="is_initial" valueBoolean="true"/>
            <column name="is_end" valueBoolean="false"/>
        </insert>

        <insert tableName="workflow_state">
            <column name="name" value="In Progress"/>
            <column name="workflow_id" value="1"/>
            <column name="is_initial" valueBoolean="false"/>
            <column name="is_end" valueBoolean="false"/>
        </insert>

        <insert tableName="workflow_state">
            <column name="name" value="In Review"/>
            <column name="workflow_id" value="1"/>
            <column name="is_initial" valueBoolean="false"/>
            <column name="is_end" valueBoolean="false"/>
        </insert>

        <insert tableName="workflow_state">
            <column name="name" value="Done"/>
            <column name="workflow_id" value="1"/>
            <column name="is_initial" valueBoolean="false"/>
            <column name="is_end" valueBoolean="true"/>
        </insert>
    </changeSet>

</databaseChangeLog>
